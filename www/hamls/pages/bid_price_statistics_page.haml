%header
  {{click_biding_name+'('+bid_peoples.length+'人)'}}
  %div.header-left.button_position
    %a.btn-style.btn-4(ng-tap="go_bid_list()" ) 返回
  %div.header-right.button_position( ng-switch='status')
    %a.btn-style.btn-4(ng-tap='go_bid_result()' ) 列表
%div.wrapper.overflow-hidden
  %ul.list-style-2
    %li.text-vertical-align(ng-repeat="price in prices")
      %div.font-size-18px
        %span.header-left
          {{'￥'+price.price}}
        %span.header-right
          {{price.number}}